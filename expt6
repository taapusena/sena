// ======================= Face Rec =========================
static mtmn_config_t mtmn_config = {
  .min_face = 80,
  .pyramid = true,
  .threshold = {
      .p_threshold = 0.6,
      .r_threshold = 0.7,
      .o_threshold = 0.7,
  }
};

// ==========================================================
//                 HTTP Server Functions
// ==========================================================

// ----------- Stream Handler (Live Video) ------------
esp_err_t stream_handler(httpd_req_t *req) {
  camera_fb_t * fb = NULL;
  esp_err_t res = ESP_OK;

  res = httpd_resp_set_type(req, "multipart/x-mixed-replace; boundary=frame");

  while(true) {
    fb = esp_camera_fb_get();
    if (!fb) break;

    // Face Detection
    dl_matrix3du_t *image_matrix = dl_matrix3du_alloc(1, fb->width, fb->height, 3);
    if (image_matrix) {
      fmt2rgb888(fb->buf, fb->len, fb->format, image_matrix->item);
      box_array_t *faces = face_detect(image_matrix, &mtmn_config);

      if (faces && faces->len > 0) {
        Serial.println("Face detected!");

        // Try recognizing
        int match = recognize_face(&id_list, image_matrix, faces);

        if (match >= 0) {
          Serial.println("FACE MATCHED â†’ Unlocking!");

          digitalWrite(UNLOCK_PIN, HIGH);
          delay(2000);
          digitalWrite(UNLOCK_PIN, LOW);
        }
      }
      dl_matrix3du_free(image_matrix);
    }

    // Send frame
    char buf[64];
    size_t hlen = snprintf((char *)buf, 64,
                     "--frame\r\nContent-Type: image/jpeg\r\n\r\n");
    httpd_resp_send_chunk(req, (const char *)buf, hlen);
    httpd_resp_send_chunk(req, (const char *)fb->buf, fb->len);
    httpd_resp_send_chunk(req, "\r\n", 2);

    esp_camera_fb_return(fb);
  }

  return res;
}


// ------------ Snapshot Handler -------------------
esp_err_t capture_handler(httpd_req_t *req){
  camera_fb_t * fb = esp_camera_fb_get();
  if (!fb) {
    httpd_resp_send_500(req);
    return ESP_FAIL;
  }

  httpd_resp_set_type(req, "image/jpeg");
  httpd_resp_send(req, (const char *)fb->buf, fb->len);

  esp_camera_fb_return(fb);
  return ESP_OK;
}


// ------------ Unlock Handler -------------------
esp_err_t unlock_handler(httpd_req_t *req){
  digitalWrite(UNLOCK_PIN, HIGH);
  delay(2000);
  digitalWrite(UNLOCK_PIN, LOW);

  httpd_resp_sendstr(req, "Door Unlocked!");
  return ESP_OK;
}


// ======================= SERVER SETUP =====================
httpd_handle_t start_webserver(void){
  httpd_config_t config = HTTPD_DEFAULT_CONFIG();
  httpd_handle_t server = NULL;

  if (httpd_start(&server, &config) == ESP_OK) {

    // Main Page
    httpd_uri_t main_page = {
      .uri = "/",
      .method = HTTP_GET,
      .handler = [](httpd_req_t *req){
        httpd_resp_set_type(req, "text/html");
        return httpd_resp_send(req, MAIN_page, strlen(MAIN_page));
      }
    };
    httpd_register_uri_handler(server, &main_page);

    // Stream
    httpd_uri_t stream_uri = {
      .uri = "/stream",
      .method = HTTP_GET,
      .handler = stream_handler
    };
    httpd_register_uri_handler(server, &stream_uri);

    // Capture
    httpd_uri_t capture_uri = {
      .uri = "/capture",
      .method = HTTP_GET,
      .handler = capture_handler
    };
    httpd_register_uri_handler(server, &capture_uri);

    // Unlock
    httpd_uri_t unlock_uri = {
      .uri = "/unlock",
      .method = HTTP_GET,
      .handler = unlock_handler
    };
    httpd_register_uri_handler(server, &unlock_uri);
  }

  return server;
}


// ======================== SETUP ============================
void setup(){
  WRITE_PERI_REG(RTC_CNTL_BROWN_OUT_REG, 0);

  Serial.begin(115200);
  Serial.setDebugOutput(true);

  // Configure unlock relay pin
  pinMode(UNLOCK_PIN, OUTPUT);
  digitalWrite(UNLOCK_PIN, LOW);

  // Camera Config
  camera_config_t config;
  config.ledc_channel = LEDC_CHANNEL_0;
  config.ledc_timer   = LEDC_TIMER_0;

  config.pin_d0 = Y2_GPIO_NUM;
  config.pin_d1 = Y3_GPIO_NUM;
  config.pin_d2 = Y4_GPIO_NUM;
  config.pin_d3 = Y5_GPIO_NUM;
  config.pin_d4 = Y6_GPIO_NUM;
  config.pin_d5 = Y7_GPIO_NUM;
  config.pin_d6 = Y8_GPIO_NUM;
  config.pin_d7 = Y9_GPIO_NUM;

  config.pin_xclk = XCLK_GPIO_NUM;
  config.pin_pclk = PCLK_GPIO_NUM;
  config.pin_vsync = VSYNC_GPIO_NUM;
  config.pin_href = HREF_GPIO_NUM;

  config.pin_sscb_sda = SIOD_GPIO_NUM;
  config.pin_sscb_scl = SIOC_GPIO_NUM;

  config.pin_pwdn = PWDN_GPIO_NUM;
  config.pin_reset = RESET_GPIO_NUM;

  config.xclk_freq_hz = 20000000;
  config.pixel_format = PIXFORMAT_JPEG;

  if(psramFound()){
    config.frame_size = FRAMESIZE_VGA;
    config.jpeg_quality = 10;
    config.fb_count = 2;
  } else {
    config.frame_size = FRAMESIZE_SVGA;
    config.jpeg_quality = 12;
    config.fb_count = 1;
  }

  // Init Camera
  esp_err_t err = esp_camera_init(&config);
  if (err != ESP_OK) {
      Serial.printf("Camera init failed: 0x%x", err);
      return;
  }

  // Add one dummy face ID for matching
  id0.id = 0;
  id_list.id_list = &id0;

  // WiFi Connect
  WiFi.begin(ssid, password);
  Serial.print("Connecting");

  while(WiFi.status() != WL_CONNECTED){
    Serial.print(".");
    delay(300);
  }

  Serial.println("\nWiFi Connected!");
  Serial.println(WiFi.localIP());

  start_webserver();
}

void loop(){}



