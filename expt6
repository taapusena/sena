// ================= WiFi Login ====================
const char* ssid = "";
const char* password = "";

// ============ Door Unlock Relay Pin ==============
#define UNLOCK_PIN 12     // connect relay/LED here

// ================= Face Recognition ==============
static face_id_list id_list = {
    .count = 1,
    .size = 1,
    .id_list = nullptr
};

static face_id_node id0;

// Forward declarations
esp_err_t stream_handler(httpd_req_t *req);
esp_err_t capture_handler(httpd_req_t *req);
esp_err_t unlock_handler(httpd_req_t *req);


