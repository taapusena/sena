#include "esp_camera.h"
#include <WiFi.h>
#include <AsyncTCP.h>
#include <ESPAsyncWebServer.h>

#include "board_config.h"   // Your camera pin file

const char *ssid = "";
const char *password = "";

// Create Web Server
AsyncWebServer server(80);

// ========== HTML PAGE USED IN THE UI ==========
const char WebPage[] PROGMEM = R"rawliteral(
